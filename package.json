{
  "name": "@outofaxis/fugo-ui-kit",
  "version": "14.1.0",
  "description": "React components Kit for Fugo",
  "scripts": {
    "build": "npm-run-all -p -l \"build:*\"",
    "build:declarations": "tsc -p ./tsconfig.build.json",
    "build:assets": "copy \"./src/**/*.svg\" ./lib/ && copy \"./src/**/*.css\" ./lib/",
    "build:styles": "postcss src/tailwind.css -o src/styles.css",
    "build:babel": "babel --extensions .ts,.tsx src --ignore \"**/*.test.tsx\" --ignore \"**/*.stories.tsx\" -d lib",
    "prepublish": "npm run build",
    "preversion": "npm run prettier && npm run lint && npm run test",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "lint": "eslint -c ./package.json ./src",
    "prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "start": "npm-run-all -p -l \"start:*\" storybook",
    "start:declarations": "tsc -p ./tsconfig.build.json --watch",
    "start:build": "babel --watch --extensions .ts,.tsx src --ignore \"**/*.test.tsx\" --ignore \"**/*.stories.tsx\" -d lib",
    "start:yalc": "chokidar \"./lib/**/*\" -c \"yalc push\"",
    "start:styles": "TAILWIND_MODE=watch postcss src/tailwind.css -o src/styles.css --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run build:styles && storybook build",
    "chromatic": "npx chromatic --project-token=180e455a4e4b"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@date-io/date-fns": "^1",
    "@fortawesome/fontawesome-svg-core": "^6",
    "@fortawesome/free-solid-svg-icons": "^6",
    "@fortawesome/react-fontawesome": "^0.2",
    "@material-ui/core": "^4",
    "@material-ui/pickers": "^3",
    "@outofaxis/react-google-flight-datepicker": "^0.9",
    "@radix-ui/react-dropdown-menu": "^2",
    "@radix-ui/react-label": "^2",
    "@radix-ui/react-portal": "^1",
    "@radix-ui/react-select": "^1",
    "@radix-ui/react-slot": "^1",
    "@radix-ui/react-tooltip": "^1",
    "@reach/checkbox": "^0.18",
    "@reach/combobox": "^0.18",
    "@reach/dialog": "^0.18",
    "@reach/disclosure": "^0.18",
    "@reach/popover": "^0.18",
    "@reach/rect": "^0.18",
    "@reach/slider": "^0.18",
    "@reach/tabs": "^0.18",
    "animate.css": "^4",
    "date-fns": "^2",
    "final-form": "^4",
    "i18next": "^21",
    "lodash": "^4",
    "react": "^18",
    "react-device-detect": "^2",
    "react-dom": "^18",
    "react-final-form": "^6",
    "react-i18next": "^11",
    "react-keyed-flatten-children": "^2",
    "react-router-dom": "^6",
    "rifm": "^0.12",
    "tailwindcss": "^3",
    "use-debounce": "^9",
    "use-media": "^1",
    "uuid": "^9"
